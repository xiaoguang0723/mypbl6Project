
.web{
	display: flex;
	flex-direction: column;
	align-items: center;
	padding: 20px 0;
}
.weblist{
	width: 80%;
	height: 50px;
	background-color: #4CD964;
	border-radius: 999rpx;
	margin-top: 15px;
	font-size: 36rpx;
	color: #fff;
	text-align: center;
	line-height: 50px;
	box-shadow: 1px 1px 3px #EEEEEE;
}
.hover{
	opacity: 0.8;
}

