<view class="pro"><view class="top"><scroll-view class="scroll_top" show-scrollbar="false" scroll-with-animation="true" scroll-x="true" scroll-into-view="{{scrollIntoView}}"><block wx:for="{{tabsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tabs" id="{{'tabId'+index}}"><text data-event-opts="{{[['tap',[['changeTabs',[index]]]]]}}" class="{{['tabtxt',tabIndex==index?'active':'']}}" bindtap="__e">{{item.name}}</text></view></block></scroll-view></view><view class="bot"><swiper class="swiper" current="{{tabIndex}}" indicator-dots="{{false}}" autoplay="{{false}}" interval="{{3000}}" duration="{{1000}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{tabsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="swiper_item"><list class="list" vue-id="{{'c3f6e438-1-'+index}}" loadmoreoffset="15" data-event-opts="{{[['^loadmore',[['loadMore',[index]]]]]}}" bind:loadmore="__e" bind:__l="__l" vue-slots="{{['default']}}"><refresh class="refresh" vue-id="{{('c3f6e438-2-'+index)+','+('c3f6e438-1-'+index)}}" display="{{refreshing?'show':'hide'}}" data-event-opts="{{[['^refresh',[['onrefresh',[index]]]],['^pullingdown',[['onpullingdown']]]]}}" bind:refresh="__e" bind:pullingdown="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:if="{{refreshing}}"><loading-indicator class="loading-icon" vue-id="{{('c3f6e438-3-'+index)+','+('c3f6e438-2-'+index)}}" animating="true" bind:__l="__l"></loading-indicator></block><text class="loading_txt">{{refreshTxt}}</text></refresh><block wx:for="{{contentList}}" wx:for-item="dataitem" wx:for-index="dataindex" wx:key="dataindex"><cell class="cell" vue-id="{{('c3f6e438-4-'+index+'-'+dataindex)+','+('c3f6e438-1-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}"><list-item vue-id="{{('c3f6e438-5-'+index+'-'+dataindex)+','+('c3f6e438-4-'+index+'-'+dataindex)}}" dataItem="{{dataitem}}" dataindex="{{dataindex}}" bind:__l="__l"></list-item></cell></block><block wx:if="{{isLoading}}"><cell class="loading-more" vue-id="{{('c3f6e438-6-'+index)+','+('c3f6e438-1-'+index)}}" bind:__l="__l" vue-slots="{{['default']}}"><text class="loading-more-text">{{loadingText}}</text></cell></block></list></swiper-item></block></swiper></view></view>