
.wrap{
		height: 100vh;
		background: #efefef;
		display: flex;
		flex-direction: column;
		justify-content: space-around;
		align-items: center;
		padding-bottom: 50px;
		color: #333;
}
.txt{
		padding: 0 25px;
		text-align: center;
		font-size: 48rpx;
}
.top{
	width: 100px;
	height: 100px;
	border-radius: 50%;
	overflow: hidden;
	margin: 0 auto;
	margin-top: 50px;
}
.top image{
	width: 100%;
	height: 100%;
}

