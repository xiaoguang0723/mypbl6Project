<view class="register"><view class="head"><view class="header"><view class="imgs"></view><view class="txt">注册</view></view></view><view class="content"><view class="main"><view class="name"><text>用户：</text><input type="text" adjust-position="true" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="password"><text>密码：</text><input type="text" password="true" adjust-position="true" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','passWord','$event',[]]]]]]}}" value="{{passWord}}" bindinput="__e"/></view><view class="re_password"><text>确认密码：</text><input class="re_password_input" type="text" password="true" adjust-position="true" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','rePassWord','$event',[]]]]]]}}" value="{{rePassWord}}" bindinput="__e"/></view></view></view><view class="register_btn"><button type="warn" data-event-opts="{{[['tap',[['registerNow']]]]}}" bindtap="__e">立即注册</button><view class="to_login">已有账号?<text data-event-opts="{{[['tap',[['to_login']]]]}}" bindtap="__e">去登录</text></view></view></view>